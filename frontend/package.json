{
  "name": "tududi-frontend",
  "private": true,
  "version": "0.86.1",
  "description": "Frontend for tududi task management",
  "scripts": {
    "dev": "cd .. && webpack serve --config webpack.config.js --hot",
    "start": "tsc --noEmit && cd .. && webpack serve --config webpack.config.js",
    "build": "pnpm run clean && tsc --noEmit && cd .. && webpack --config webpack.config.js",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "test:coverage": "jest --config jest.config.js --coverage",
    "lint": "cd .. && eslint 'frontend/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "cd .. && eslint --fix 'frontend/**/*.{js,jsx,ts,tsx}'",
    "format": "cd .. && prettier -c 'frontend/**/*.{js,jsx,ts,tsx}'",
    "format:fix": "cd .. && prettier --write 'frontend/**/*.{js,jsx,ts,tsx}'",
    "clean": "rimraf ../dist"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@heroicons/react": "^2.1.5",
    "@yaireo/tagify": "^4.31.3",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "highlight.js": "^11.11.1",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.0.4",
    "i18next-http-backend": "^3.0.2",
    "linguaisync": "^0.1.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.4.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.26.2",
    "react-tagify": "^1.0.7",
    "recharts": "^2.15.4",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "slugify": "^1.6.6",
    "swr": "^2.2.5",
    "tagify": "^0.1.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "@babel/preset-react": "^7.25.7",
    "@babel/preset-typescript": "^7.25.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@swc/core": "^1.13.3",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "autoprefixer": "^10.4.20",
    "babel-jest": "^29.0.0",
    "babel-loader": "^9.2.1",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "eslint": "^8.0.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^15.11.0",
    "html-webpack-plugin": "^5.6.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "postcss": "^8.4.47",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.6.2",
    "react-refresh": "^0.14.2",
    "rimraf": "^6.0.1",
    "style-loader": "^4.0.0",
    "swc-loader": "^0.2.6",
    "tailwindcss": "^3.4.13",
    "ts-jest": "^29.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.36.0",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  }
}
