    "react-i18next": "^15.4.1",
    "react-markdown": "^10.1.0",
    "react-refresh": "^0.14.2",
    "react-router-dom": "^6.26.2",
    "react-tagify": "^1.0.7",
    "recharts": "^2.15.4",
